<!DOCTYPE html>
<html>
<head>
  <meta charset="utf-8">
  <title>会员编辑</title>
  <meta name="renderer" content="webkit">
  <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
  <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">
    {include file="public/css"}
    {include file="public/js"}
</head>
<body>



<div class=""  style="padding:20px">
<form class="layui-form" action="">

<table>
  <colgroup>
    <col width="50%">
    <col width="50%">
    <col>
  </colgroup>
  <tbody>
  <tr> 
 <td><div class="layui-form-item">
                <label class="layui-form-label" style="width:50px;">头像</label>
                <div class="layui-input-inline">
                  <img src=""  style="width:60px;height:60px">
                </div>
  </div>
 </td>
   <td>
       <div class="layui-form-item">
                <label class="layui-form-label" style="width:50px;">用户名</label>
                <div class="layui-input-block">
                 {$member.username}
                </div>
        </div>

    </td>

  </tr>

    <tr>
      <td>
              <div class="layui-form-item">
                <label class="layui-form-label" style="width:50px;">用户名</label>
                <div class="layui-input-block">
                 {$member.username}
                </div>
              </div>
              <div class="layui-form-item">
                <label class="layui-form-label" style="width:50px;">余额</label>
                <div class="layui-input-block">
               {$member.money} {$settings.site.money_unit}
                </div>
              </div>
              <div class="layui-form-item">
                <label class="layui-form-label" style="width:50px;">积分</label>
                <div class="layui-input-block">
                 {$member.credit} 
                </div>
              </div>

  </td>
  <td>  

              <div class="layui-form-item">
                <label class="layui-form-label" style="width:50px;">用户组</label>
                <div class="layui-input-inline">
                  <input type="text" name="title" lay-verify="title" autocomplete="off" placeholder="" class="layui-input">
                </div>
              </div>
              <div class="layui-form-item">
                <label class="layui-form-label" style="width:50px;">性别</label>
                <div class="layui-input-inline">
                  <input type="text" name="title" lay-verify="title" autocomplete="off" placeholder="" class="layui-input">
                </div>
              </div>
              <div class="layui-form-item">
                <label class="layui-form-label" style="width:50px;">登陆密码</label>
                <div class="layui-input-inline">
                  <input type="text" name="title" lay-verify="title" autocomplete="off" placeholder="" class="layui-input">
                </div>
              </div>
              <div class="layui-form-item">
                <label class="layui-form-label" style="width:50px;">支付密码</label>
                <div class="layui-input-inline">
                  <input type="text" name="title" lay-verify="title" autocomplete="off" placeholder="" class="layui-input">
                </div>
              </div>
              <div class="layui-form-item">
                <label class="layui-form-label" style="width:50px;">登陆密码</label>
                <div class="layui-input-inline">
                  <input type="text" name="title" lay-verify="title" autocomplete="off" placeholder="" class="layui-input">
                </div>
              </div>
              <div class="layui-form-item">
                <label class="layui-form-label" style="width:50px;">支付密码</label>
                <div class="layui-input-inline">
                  <input type="text" name="title" lay-verify="title" autocomplete="off" placeholder="" class="layui-input">
                </div>
              </div>
              <div class="layui-form-item">
                <label class="layui-form-label" style="width:50px;">登陆密码</label>
                <div class="layui-input-inline">
                  <input type="text" name="title" lay-verify="title" autocomplete="off" placeholder="" class="layui-input">
                </div>
              </div>
              <div class="layui-form-item">
                <label class="layui-form-label" style="width:50px;">支付密码</label>
                <div class="layui-input-inline">
                  <input type="text" name="title" lay-verify="title" autocomplete="off" placeholder="" class="layui-input">
                </div>
              </div>
              </td>
    </tr>
  </tbody>
</table>
 <button class="layui-btn layui-btn-normal">确 定</button>
</form>
</div>
<script src="__STATIC__/layui/layui.js" charset="utf-8"></script>
